var convertLifeStyleType = function (type) {
  switch (type) {
    case 'comf':
      return '舒适度指数';
    case 'cw':
      return '洗车指数';
    case 'drsg':
      return '穿衣指数';
    case 'flu':
      return '感冒指数';
    case 'sport':
      return '运动指数';
    case 'trav':
      return '旅游指数';
    case 'uv':
      return '紫外线指数';
    case 'air':
      return '空气污染扩散条件指数';
    case 'ac':
      return '空调开启指数';
    case 'ag':
      return '过敏指数';
    case 'gl':
      return '太阳镜指数';
    case 'mu':
      return '化妆指数';
    case 'airc':
      return '晾晒指数';
    case 'ptfc':
      return '交通指数';
    case 'fsh':
      return '钓鱼指数';
    case 'spi':
      return '防晒指数';
  }
}
var sliceArr = function(arr, start=0, end=6) {
  if(!arr || !arr.length) {
    return []
  }
  // console.log(arr)
  console.log(JSON.stringify(arr.slice(start, end)))
  return arr.slice(start, end)
}

module.exports = {
  convertLifeStyleType: convertLifeStyleType,
  sliceArr: sliceArr
}